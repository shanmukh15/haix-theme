{
  "extends": ["stylelint-config-recommended"],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "tailwind",
          "apply",
          "variants",
          "responsive",
          "screen",
          "extends",
          "components",
          "utilities"
        ]
      }
    ],
    "no-descending-specificity": null
  }
}
